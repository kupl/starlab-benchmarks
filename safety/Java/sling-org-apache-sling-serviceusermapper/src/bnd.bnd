Import-Package:\
  org.apache.felix.inventory;resolution:=optional,\
  *

-includeresource:\
  @org.apache.felix.utils-*.jar!/org/apache/felix/utils/json/JSONWriter*

-removeheaders:\
  Include-Resource,\
  Private-Package

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE facelet-taglib PUBLIC
    "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
    "http://java.sun.com/dtd/facelet-taglib_1_0.dtd" >

<facelet-taglib>
	<namespace>http://stella.caelum.com.br/faces</namespace>
	<tag>
		<tag-name>validateIE</tag-name>
		<validator>
			<validator-id>ieValidator</validator-id>
			<handler-class>
				br.com.caelum.stella.faces.validation.StellaIEValidatorTagHandler
			</handler-class>
		</validator>
	</tag>
	<tag>
		<tag-name>validateCPF</tag-name>
		<validator>
			<validator-id>cpfValidator</validator-id>
			<handler-class>
				br.com.caelum.stella.faces.validation.StellaCPFValidatorTagHandler
			</handler-class>
		</validator>
	</tag>
	<tag>
		<tag-name>inputCPF</tag-name>
		<source>inputCPF.xhtml</source>
	</tag>
	<tag>
		<tag-name>validateCNPJ</tag-name>
		<validator>
			<validator-id>cpnjValidator</validator-id>
			<handler-class>
				br.com.caelum.stella.faces.validation.StellaCNPJValidatorTagHandler
			</handler-class>
		</validator>
	</tag>
	<tag>
		<tag-name>validateTituloEleitoral</tag-name>
		<validator>
			<validator-id>tituloEleitoralValidator</validator-id>
			<handler-class>
				br.com.caelum.stella.faces.validation.StellaTituloEleitoralValidatorTagHandler
			</handler-class>
		</validator>
	</tag>
	<tag>
		<tag-name>validateNIT</tag-name>
		<validator>
			<validator-id>nitValidator</validator-id>
			<handler-class>
				br.com.caelum.stella.faces.validation.StellaNITValidatorTagHandler
			</handler-class>
		</validator>
	</tag>
</facelet-taglib>

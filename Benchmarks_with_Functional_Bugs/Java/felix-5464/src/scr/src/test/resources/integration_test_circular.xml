        </service>
        <reference
            name="a"
            interface="org.apache.felix.scr.integration.components.circular.A"
            cardinality="1..1"
            policy="dynamic"
            bind="setA"
            unbind="unsetA"
            target="(service.pid=1.A.1.1.dynamic)"
        />
        <property name="service.pid" value="1.B.1.1.dynamic" />
    </scr:component>

